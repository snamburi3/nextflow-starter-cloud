profiles {
    process.cache = 'lenient'
    gcp {
        executor {
            name = 'google-lifesciences'
        }
        //process.executor = 'google-lifesciences'
        docker {
            enabled = true
            temp = 'auto'
        }
        google.project = 'PROJECT_ID'
        google.location = 'PROJECT_LOCATION'
        google.region  = 'PROJECT_REGION'
        google.lifeSciences.debug = true
        google.lifeSciences.preemptible = true
        google.lifeSciences.usePrivateAddress = true
        google.lifeSciences.sshDaemon = true
        google.lifeSciences.bootDiskSize = 100.GB
        google.enableRequesterPaysBuckets = true
        process {
            container = 'us.gcr.io/PROJECT_ID/ubuntu:test'
		}
    }
}
